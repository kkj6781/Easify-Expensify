{
    "manifest_version": 2,
    "name": "Expensify $14/day",
    "version": "0.1",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://*.expensify.com/",
        "https://*.expensify.com/",
        "storage"        
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}